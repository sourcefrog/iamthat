{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "description": "An IAM user.",
  "type": "object",
  "required": [
    "UserName"
  ],
  "properties": {
    "AttachedPolicies": {
      "description": "Named policies attached to this user.",
      "default": [],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "Path": {
      "description": "The user's path.",
      "default": "/",
      "type": "string"
    },
    "Tags": {
      "description": "The user's tags.",
      "default": [],
      "type": "array",
      "items": {
        "$ref": "#/definitions/Tag"
      }
    },
    "UserId": {
      "description": "The user's id.",
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "UserName": {
      "description": "The user's name.",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "definitions": {
    "Tag": {
      "description": "A key-value tag.",
      "type": "object",
      "required": [
        "Key",
        "Value"
      ],
      "properties": {
        "Key": {
          "type": "string"
        },
        "Value": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
